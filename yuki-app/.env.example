# Yuki App - Environment Variables
# Copy this to .env and fill in your values

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=yuki-ai-app
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=G-XXXXXXX

# GCP Configuration
EXPO_PUBLIC_GCP_PROJECT_ID=who-visions
EXPO_PUBLIC_GCS_BUCKET=yuki-ai-transformations

# API Endpoint (for signed URLs, etc.)
EXPO_PUBLIC_API_URL=https://api.yukiai.app
